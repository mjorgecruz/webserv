http{
    server {
        listen      80;
        server_name example.org www.example.org;

    }

    server {
        listen      80;
        server_name example.net www.example.net;
        ...
    }

    server {
        listen      80;
        server_name example.com www.example.com;
        ...
    }

    server {
        listen      80;
        server_name "";
        allow_methods GET POST DELETE;

        root /var/

        location /{
            method GET, POST
            root /var/
            index index.html index.htp index.a
            
        }
        location /*.php
        {
            cgi_path /usr/bin/php-cgi;
            allow_methods GET POST DELETE;
            cgi_extension .php;
        }


    }
}